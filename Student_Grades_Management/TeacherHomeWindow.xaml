<Window x:Class="Student_Grades_Management.TeacherHomeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Student_Grades_Management"
        mc:Ignorable="d"
        Title="TeacherHomeWindow" Height="450" Width="800">
    <Grid>
        <DockPanel LastChildFill="False">
            <Menu x:Name="mnuMain" DockPanel.Dock="Top" Height="20">
                <MenuItem x:Name="mnuViewTeaching" Header="Teaching Subjects" Click="mnuViewTeaching_Click"></MenuItem>
                <MenuItem x:Name="mnuProfile" Header="Profile" Click="mnuProfile_Click"></MenuItem>
                <MenuItem x:Name="mnuLogin" Header="Logout" Click="mnuLogin_Click"></MenuItem>
            </Menu>
        </DockPanel>
    </Grid>
</Window>
