<Window x:Class="Student_Grades_Management.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Student_Grades_Management"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DockPanel LastChildFill="False">
            <Menu x:Name="mnuMain" DockPanel.Dock="Top" Height="20">
                <MenuItem x:Name="mnuSWP301" Header="SWP301" Click="mnuShow_SWP391"></MenuItem>
                <MenuItem x:Name="mnuSWT301" Header="SWT301" Click="mnuShow_SWT301"></MenuItem>
                <MenuItem x:Name="mnuSWR302" Header="SWR302" Click="mnuShow_SWR302"></MenuItem>
                <MenuItem x:Name="mnuPRN212" Header="PRN212" Click="mnuShow_PRN212"></MenuItem>
                <MenuItem x:Name="mnuProfile" Header="Profile" Click="mnuProfile_Click"></MenuItem>
                <MenuItem x:Name="mnuLogin" Header="Logout" Click="mnuLogin_Click"></MenuItem>
            </Menu>
        </DockPanel>
    </Grid>
</Window>
